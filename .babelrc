{
    "presets": [
        [
            "@babel/preset-env",
            {
                "debug": true,
                "loose": true,
                "modules": false,
                "useBuiltIns": false
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-syntax-dynamic-import",
        [
            "@babel/plugin-proposal-object-rest-spread",
            {
                "useBuiltIns": false
            }
        ]
    ]
}
